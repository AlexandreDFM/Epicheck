{
  "name": "epicheck",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "ios:simulator": "expo run:ios",
    "build:ios": "eas build --platform ios",
    "build:ios:local": "eas build --platform ios --local",
    "web": "expo start --web",
    "proxy": "cd proxy-server && node server.js",
    "build:android:debug": "expo run:android --variant debug",
    "build:android:release": "cd android && ./gradlew assembleRelease --warning-mode all && cd ..",
    "build:apk": "cd android && ./gradlew assembleRelease --warning-mode all && cd ..",
    "lint": "expo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md,yaml}\""
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-cookies/cookies": "^6.2.1",
    "@react-navigation/native": "^7.1.22",
    "@react-navigation/native-stack": "^7.8.2",
    "axios": "^1.13.2",
    "babel-preset-expo": "~54.0.10",
    "expo": "^54.0.33",
    "expo-audio": "~1.1.1",
    "expo-auth-session": "~7.0.10",
    "expo-camera": "~17.0.10",
    "expo-crypto": "~15.0.8",
    "expo-document-picker": "~14.0.8",
    "expo-font": "~14.0.11",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "^6.0.9",
    "expo-web-browser": "~15.0.10",
    "moment": "^2.30.1",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-css-interop": "^0.2.1",
    "react-native-nfc-manager": "^3.17.2",
    "react-native-reanimated": "~4.1.5",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "^0.21.2",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "react-native-worklets-core": "^1.6.2",
    "tailwindcss": "^3.4.18"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@types/react": "~19.1.17",
    "babel-preset-expo": "^54.0.7",
    "eslint": "^9.39.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "metro": "^0.83.3",
    "metro-core": "^0.83.3",
    "prettier": "^3.7.3",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "~5.9.3"
  },
  "private": true
}
